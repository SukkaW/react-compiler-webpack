{
  "name": "react-compiler-webpack",
  "version": "1.0.0",
  "description": "The webpack/Next.js Plugin for React Compiler",
  "repository": "https://github.com/SukkaW/react-compiler-webpack",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "next.js"
  ],
  "scripts": {
    "lint": "eslint --format=sukka .",
    "build": "rollup -c rollup.config.ts --configPlugin swc3 --bundleConfigAsCjs",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "mocha -r @swc-node/register --require mocha-expect-snapshot 'test/index.ts'",
    "test:update": "UPDATE_SNAPSHOT=all mocha -r @swc-node/register --require mocha-expect-snapshot 'test/index.ts'",
    "prerelease": "pnpm run build && pnpm run lint",
    "release": "bumpp -r --all --commit \"release: %s\" --tag \"%s\""
  },
  "keywords": [
    "react",
    "react-forget",
    "react-compiler",
    "webpack-loader",
    "webpack",
    "nextjs",
    "app-router"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/plugin-syntax-typescript": "^7.27.1"
  },
  "devDependencies": {
    "@eslint-sukka/node": "^8.0.3",
    "@jest/expect": "^30.2.0",
    "@rspack/core": "^1.6.3",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.1",
    "@types/babel__core": "^7.20.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.1",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "browserslist": "^4.28.0",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.1",
    "eslint-config-sukka": "^8.0.3",
    "eslint-formatter-sukka": "^8.0.3",
    "expect": "^30.2.0",
    "memfs": "^5.0.0-next.1",
    "mocha": "^11.7.5",
    "mocha-expect-snapshot": "^8.0.0",
    "rollup": "^4.53.2",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-swc3": "^0.12.1",
    "swc-loader": "^0.2.6",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1"
  },
  "peerDependencies": {
    "babel-plugin-react-compiler": "*"
  },
  "packageManager": "pnpm@10.22.0",
  "pnpm": {
    "neverBuiltDependencies": [],
    "overrides": {
      "eslint>chalk": "npm:picocolors@^1.1.1"
    }
  },
  "overrides": {
    "hasown": "npm:@nolyfill/hasown@latest"
  }
}
