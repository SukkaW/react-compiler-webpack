{
  "name": "react-compiler-webpack",
  "version": "0.1.2",
  "description": "The webpack/Next.js Plugin for React Compiler",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "next.js"
  ],
  "scripts": {
    "lint": "eslint --format=sukka .",
    "build": "rollup -c rollup.config.ts --configPlugin swc3 --bundleConfigAsCjs",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "mocha -r @swc-node/register 'test/index.ts'",
    "test:update": "mocha --update -r @swc-node/register 'test/index.ts'"
  },
  "keywords": [
    "react",
    "react-forget",
    "react-compiler",
    "webpack-loader",
    "webpack",
    "nextjs",
    "app-router"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-jsx": "^7.24.7",
    "@babel/plugin-syntax-typescript": "^7.25.4",
    "loader-utils": "^3.3.1"
  },
  "devDependencies": {
    "@eslint-sukka/node": "^6.2.2",
    "@rspack/core": "^0.7.5",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.7.22",
    "@types/babel__core": "^7.20.5",
    "@types/chai": "^4.3.19",
    "@types/loader-utils": "^2.0.6",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.16.2",
    "babel-plugin-react-compiler": "0.0.0-experimental-c23de8d-20240515",
    "browserslist": "^4.23.3",
    "bumpp": "^9.5.2",
    "chai": "^4.5.0",
    "eslint": "^9.9.1",
    "eslint-config-sukka": "^6.2.2",
    "eslint-formatter-sukka": "^6.2.2",
    "memfs": "^5.0.0-next.1",
    "mocha": "^10.7.3",
    "mocha-chai-jest-snapshot": "^1.1.5",
    "next": "^14.2.7",
    "postcss": "^8.4.41",
    "rimraf": "^5.0.10",
    "rollup": "^4.21.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-swc3": "^0.11.2",
    "swc-loader": "^0.2.6",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0"
  },
  "peerDependencies": {
    "babel-plugin-react-compiler": "*"
  },
  "packageManager": "pnpm@9.9.0",
  "overrides": {
    "hasown": "npm:@nolyfill/hasown@latest",
    "chai": "$chai"
  }
}
